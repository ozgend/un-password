{
  "name": "un-password",
  "author": "denolk",
  "description": "simple way to reveal password inputs on chrome - for lazies",
  "version": "1.0.2",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-32.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "manifest_version": 2
}